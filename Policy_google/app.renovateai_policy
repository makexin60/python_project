                                                                                                                                                                                                                                                                                a
                                                                                                                                                                                                                                                                                standard
                                                                                                                                                                                                                                                                                for
                                                                                                                                                                                                                                                                                online
                                                                                                                                                                                                                                                                                tracking
                                                                                                                                                                                                                                                                                is
                                                                                                                                                                                                                                                                                adopted
                                                                                                                                                                                                                                                                                that
                                                                                                                                                                                                                                                                                we
                                                                                                                                                                                                                                                                                must
                                                                                                                                                                                                                                                                                follow
                                                                                                                                                                                                                                                                                in
                                                                                                                                                                                                                                                                                the
