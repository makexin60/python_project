                                                                                                                                                                                                                                                                                                                Regulations
                                                                                                                                                                                                                                                                                                                defines
                                                                                                                                                                                                                                                                                                                a
"resident"
as:
(1)
                                                                                                                                                                                                                                                                                every
                                                                                                                                                                                                                                                                                individual
                                                                                                                                                                                                                                                                                who
                                                                                                                                                                                                                                                                                is
